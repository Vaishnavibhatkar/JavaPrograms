package Pattern1;

public class star14 {

	public static void main(String[] args)
	{
		for(int i=0;i<=4;i++)
		{
			for(int j=0;j<=4;j++)
			{
				if(i==j || i+j==0)
				{
					System.out.println(" ");
				}
				else
				{
					System.out.println("*");
				}
			}
			System.out.println();
			
		}


	}

}
