package Collection;
import java.util.ArrayList;

public class Array_List 
{

	public static void main(String[] args) 
	{
		ArrayList al = new ArrayList();
		al.add(100);
		al.add("Vaishu");
		al.add('A');
		al.add(1000);
		
		al.remove(0);
		System.out.println(al);
		
		System.out.println(al);
		
		ArrayList al1 = new ArrayList();
		al1.add(200);
		al1.add("Mahi");
		al1.add('A');
		al1.add(2000);
		
		al.addAll(al1);
		
		System.out.println(al);
		

	}

}
