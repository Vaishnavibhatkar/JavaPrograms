package Pattern1;

public class star16 {

	public static void main(String[] args) 
	{
	        int a=0 , b= 8;
	        for(int i=0; i<=11; i++)
	        {
	        	for(int j=0; j<6; j++)
	        	{
	        		if(j-i>0 || j+i>=10)
	        		{
	        			if(a<=9)
	        			{
	        				System.out.println(a+" ");
	        			}
	        			else if (a>33)
	        			{
	        				System.out.println(b+" ");
	        				b--;
	        			}
	        			else if(a%2==0 && a>9)
	        			{
	        				System.out.println("0");
	        			}
	        			else
	        			{
	        				System.out.println("9 ");
	        			}
	        		}
	        	}
	        }

	}

}
