package HelloJava;
import java.util.*;

public class UserInput {

	public static void main(String[] args) 
	{
	  Scanner Sc = new Scanner (System.in);
	  System.out.println(" Enter The Full Name:");
	  String Name =Sc.next();
	  System.out.println("--------------------------------------");
	  System.out.println("Name:" +Name);

	}

}
